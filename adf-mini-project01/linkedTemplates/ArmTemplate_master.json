{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-mini-project01"
		},
		"ls_blob_storage_mini_project_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_storage_mini_project'"
		},
		"ls_sqldb_mini_project_2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqldb_mini_project_2'"
		},
		"ls_sqldb_set1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqldb_set1'"
		},
		"ls_sqldb_mini_project_2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ds-data-training.database.windows.net"
		},
		"ls_sqldb_mini_project_2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_mini_project_usecase_two"
		},
		"ls_sqldb_mini_project_2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vijaydrravi"
		},
		"ls_sqldb_set1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ds-data-training.database.windows.net"
		},
		"ls_sqldb_set1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-mini-project"
		},
		"ls_sqldb_set1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vijaydrravi"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-mini-project01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_storage_mini_project_connectionString": {
						"value": "[parameters('ls_blob_storage_mini_project_connectionString')]"
					},
					"ls_sqldb_mini_project_2_password": {
						"value": "[parameters('ls_sqldb_mini_project_2_password')]"
					},
					"ls_sqldb_set1_password": {
						"value": "[parameters('ls_sqldb_set1_password')]"
					},
					"ls_sqldb_mini_project_2_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldb_mini_project_2_properties_typeProperties_server')]"
					},
					"ls_sqldb_mini_project_2_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldb_mini_project_2_properties_typeProperties_database')]"
					},
					"ls_sqldb_mini_project_2_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqldb_mini_project_2_properties_typeProperties_userName')]"
					},
					"ls_sqldb_set1_properties_typeProperties_server": {
						"value": "[parameters('ls_sqldb_set1_properties_typeProperties_server')]"
					},
					"ls_sqldb_set1_properties_typeProperties_database": {
						"value": "[parameters('ls_sqldb_set1_properties_typeProperties_database')]"
					},
					"ls_sqldb_set1_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqldb_set1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-mini-project01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-mini-project01_ArmTemplate_0')]"
			]
		}
	]
}